<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bernardthompson_assignment1">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="main.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RingfortAllMapsActivity"
            android:label="@string/title_activity_ringfort_all_maps"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".testNav"
            android:label="@string/title_activity_test_nav" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".RingfortMapsActivity"
            android:label="@string/title_activity_ringfort_maps" />
        <activity
            android:name="activities.SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="activities.RingfortActivityList" />
        <activity android:name="activities.RingfortActivity" />
        <activity android:name="activities.UserLogin" />
        <activity android:name="activities.UserRegister" />
        <activity android:name="activities.Settings" />
    </application>

</manifest>